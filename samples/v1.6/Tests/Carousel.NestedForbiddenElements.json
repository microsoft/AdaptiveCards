								{
									"type": "TextBlock",
									"text": "Input.Text"
								},
								{
									"type": "Input.Text",
									"id": "id1"
								},
								{
									"type": "TextBlock",
									"text": "Input.Number"
								},
								{
									"type": "Input.Number",
									"id": "id2"
								},
								{
									"type": "TextBlock",
									"text": "Input.ChoiceSet"
								},
								{
									"type": "Input.ChoiceSet",
									"id": "id3",
									"choices": [
										{
											"title": "Red",
