{
  "type": "AdaptiveCard",
  "$schema": "http://adaptivecards.io/schemas/adaptive-card.json",
  "version": "1.5",
  "body": [
    {
      "type": "TextBlock",
      "text": "Disabled Show Card",
      "wrap": true
    }
  ],
  "actions": [
    {
      "type": "Action.ShowCard",
      "title": "Enabled",
      "card": {
        "type": "AdaptiveCard",
        "body": [
          {
            "type": "TextBlock",
            "text": "You can see this show card!",
            "wrap": true
          }
        ]
      }
    },
    {
      "type": "Action.ShowCard",
      "title": "Disabled",
      "card": {
        "type": "AdaptiveCard",
        "body": [
          {
            "type": "TextBlock",
            "text": "You cannot see this show card.",
            "wrap": true
          }
        ]
      },
      "isEnabled": false
    }
  ]
}
