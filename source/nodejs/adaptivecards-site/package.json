    "release": "hexo clean && hexo generate",
    "start": "hexo server"
  },
  "hexo": {
    "version": "3.9.0"
  },
  "devDependencies": {
    "@fortawesome/fontawesome-free": "^5.11.2",
    "ac-typed-schema": "^0.5.2",
