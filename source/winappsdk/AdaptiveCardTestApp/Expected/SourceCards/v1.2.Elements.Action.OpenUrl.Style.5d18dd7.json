{
	"$schema": "http://adaptivecards.io/schemas/adaptive-card.json",
	"type": "AdaptiveCard",
	"version": "1.2",
	"body": [
		{
			"type": "TextBlock",
			"text": "This card has four OpenUrl actions. The styles from left to right are \"Default\", \"Positive\", \"Destructive\" and \"Other\".",
			"wrap": true
		}
	],
	"actions": [
		{
			"type": "Action.OpenUrl",
			"style": "default",
			"url": "https://adaptivecards.io",
			"title": "Default"
		},
		{
			"type": "Action.OpenUrl",
			"style": "positive",
			"url": "https://adaptivecards.io",
			"title": "Pawsitive"
		},
		{
			"type": "Action.OpenUrl",
			"style": "destructive",
			"url": "https://adaptivecards.io",
			"title": "Destructive"
		},
		{
			"type": "Action.OpenUrl",
			"style": "other",
			"url": "https://adaptivecards.io",
			"title": "Other"
		}
	]
}
