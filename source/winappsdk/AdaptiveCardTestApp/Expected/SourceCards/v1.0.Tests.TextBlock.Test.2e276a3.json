{
	"type": "AdaptiveCard",
	"version": "1.0",
	"$schema": "http://adaptivecards.io/schemas/adaptive-card.json",
	"body": [
    {
        "type": "TextBlock",
        "text": "Below markdown link should trigger telephony service",
        "wrap": true
    },
    {
        "type": "TextBlock",
        "text": "[(555) 555-5555](tel:5555555555)"
    },
	{
		"items": [
		{
			"text": "The ColumnSet below has a column that has a TextBlock witout the required property, \"text\", Renderers should render this card as rendering decision is made per element",
			"weight": "Bolder",
			"wrap" : true,
			"type": "TextBlock"
		}
		],
		"type": "Container"
	},
	{
		"items": [
		{
			"columns": [
				{
					 "items": [
					{
						"spacing": "Padding",
						"type": "Container"
					},
					{
						"type": "TextBlock"
					},
					{
						"spacing": "Padding",
						"type": "Container"
					}
					 ],
					 "type": "Column",
					 "width": "stretch"
				}
			],
			"type": "ColumnSet"
			 }
		],
		"spacing": "Small",
		"type": "Container"
	}
	]
}
